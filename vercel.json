{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*\\.(css|js|ico|jpg|png|svg))",
      "dest": "/public/$1"
    }
  ]
}